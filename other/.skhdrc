cmd - h [
  "alacritty" : skhd -k "ctrl - b" && skhd -k "h"
  "emacs" : skhd -k "ctrl - w" && skhd -k "h"
]

cmd - j [
  "alacritty" : skhd -k "ctrl - b" &&  skhd -k "j"
  "emacs" : skhd -k "ctrl - w" && skhd -k "j"
]

